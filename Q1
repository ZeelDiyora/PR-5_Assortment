#include<stdio.h>

int main(){
    
   int a[100],i,n;

    printf("Enter the array's size : ");
    scanf("%d",&n);

    printf("Enter array's elements : ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);}
    printf("All negative elements from an array are : ");
    for(i=0;i<n;i++){
        if(a[i]<0){

            printf("%d\t",a[i]);}
  }
    return 0;
}
